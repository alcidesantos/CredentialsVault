<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_launcher_name"
        android:theme="@style/Theme.CredentialsVault">

        <!-- Declaração da primeira Activity -->
        <activity
            android:name=".ListaCredenciais"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".DefinicaoCredencial"
            android:exported="false"
            android:parentActivityName=".ListaCredenciais"
            android:windowSoftInputMode="adjustResize" >
        </activity>

        <activity
            android:name=".ImpExpActivity"
            android:exported="false"
            android:parentActivityName=".ListaCredenciais" >
        </activity>

    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>